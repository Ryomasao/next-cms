    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.0.0",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.2",
    "json-server": "^0.17.0",
    "lint-staged": "^11.2.3",
    "prettier": "^2.4.1",
    "typescript": "^4.4.3"
